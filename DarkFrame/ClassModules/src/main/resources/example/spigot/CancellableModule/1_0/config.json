/* All JsonObject can be out source to a File. You must set the value of the key to a String with the ConfigPath */
{
  /* required! */
  "ConfigVersion": "1.0",
  /* not mandatory */
  "Examples": {
    /* not mandatory (default is false) */
    "NoGeneration": false,
    /* not mandatory (default is false) */
    "ResetGenerated": true,
    /* not mandatory (default is ["1SexyMap", "2SuperSexyMap", "3JoJoMap", "4IAmJoJoOfficial.org"]) */
    "WorldNames": [
      "1SexyMap",
      "2SuperSexyMap",
      "3JoJoMap",
      "4IAmJoJoOfficial.org"
    ]
  },
  /* not mandatory */
  "Messages": {
    /* not mandatory (default is "en_US") */
    "language": "de_DE",
    /* not mandatory */
    "across-languages": "messages/across-languages.json",
    /* not mandatory*/
    "languages": "messages/languages.json"
  },
  /* not mandatory */
  "LogMessages": "error-messages.json",
  /* required! */
  "Configs": {
    /* required! */
    "AsyncPlayerChat": "AsyncPlayerChat/config.json",
    /* required! */
    "CraftItem": {
      /* not mandatory */
      "BlockedMaterials": [
        "TNT"
      ]
    }
  },
  /* not mandatory */
  "Permissions": /*Use null for deny bypass Permissions*/ {
    /* not mandatory */
    "CraftItem": "CancellableModule.CraftItem.Bypass.<ItemType>",
    /* not mandatory */
    "BlockBreak": "CancellableModule.BlockBreak.Bypass.<ItemType>"
  }
}